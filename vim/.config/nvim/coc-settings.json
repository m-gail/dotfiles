{
    "rust-analyzer.server.path": "/usr/bin/rust-analyzer",
    "suggest.enablePreselect": true,
    "suggest.selection": "recentlyUsed",
    "suggest.noselect": false,
    "python": {
        "analysis": {
            "diagnosticMode": "workspace"
        },
        "formatting": {
            "provider": "black",
            "blackPath": "~/.local/bin/black"
        },
        "organizeimports": {
            "provider": "isort"
        },
        "sortImports": {
            "path": "~/.local/bin/isort"
        },
        "settings": {
            "rope_completion": {
                "enabled": true
            }
        }
    }
}
